{
  "name": "Chaskiq",
  "description": "Chaskiq Chaskiq is web platform that enables chat comunication with users in app or via campaigns (in app messages or newsletters). The platform is made with React, Redux and Ruby on Rails on backend serving a graphql api.",
  "website": "https://www.chaskiq.io/",
  "repository": "https://github.com/michelson/chaskiq",
  "logo": "data:image/png;base64,",
  "keywords": [
    "conversational marketing",
    "newsletters",
    "live chat",
    "customer support",
    "ruby",
    "rails",
    "react",
    "graphql"
  ],
  "success_url": "/",
  "scripts": {
    "postdeploy": "bundle exec rake db:schema:load && bundle exec rake db:seed"
  },
  "env": {
    "ADMIN_EMAIL": {
      "description": "An admin email to be generated on db:seed task",
      "value": ""
    },
    "ADMIN_PASSWORD": {
      "description": "An admin passwod to be generated on db:seed task",
      "generator": "secret"
    },
    "SECRET_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "RACK_ENV": {
      "description": "Environment for rack middleware.",
      "value": "production"
    },
    "RAILS_ENV": {
      "description": "Environment for rails middleware.",
      "value": "production"
    },
    "HOST": {
      "description": "App's internet entry point ie:. https://myapp.herokuapp.com",
      "value": ""
    },
    "WS": {
      "description": "App's websocket server, wss://myapp.herokuapp.com/cable",
      "value": ""
    },
    "ASSET_HOST": {
      "description": "App assets's internet entry point ie:. https://myapp.herokuapp.com",
      "value": ""
    },
    "SNS_CONFIGURATION_SET": {
      "description": "Amazon's SNS configuration set (for incoming email processing)",
      "value": ""
    },
    "AWS_ACCESS_KEY_ID": {
      "description": "Amazon's S3 file storage key",
      "value": ""
    },
    "AWS_SECRET_ACCESS_KEY": {
      "description": "Amazon's S3 file secret access key",
      "value": ""
    },
    "AWS_S3_BUCKET": {
      "description": "Amazon's S3 file storage bucket",
      "value": ""
    },
    "AWS_S3_REGION": {
      "description": "Amazon's S3 file storage region",
      "value": ""
    },
    "DEFAULT_SENDER_EMAIL": {
      "description": "A Base default email sender",
      "value": ""
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "FREE"
    }
  },
  "image": "heroku/ruby",
  "addons": [
    {
      "plan": "heroku-redis"
    },
    {
      "plan": "bugsnag:tauron2"
    },
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "11"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "environments": {
    "test": {
      "scripts":{
        "test": "bundle exec rspec",
        "test": "bundle exec rspec -t browser"
      },
      "addons":[
        "heroku-redis",
        "heroku-postgresql"
      ],
      "buildpacks": [
        { "url": "heroku/ruby" },
        { "url": "heroku/nodejs" },
        { "url": "https://github.com/heroku/heroku-buildpack-google-chrome" },
        { "url": "https://github.com/heroku/heroku-buildpack-chromedriver" }
      ]
    }
  }
}
