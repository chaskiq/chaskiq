<%= render "shared/modal" do %>
	<%= form_for @custom_field, 
		url: app_user_data_path(@app.key, @custom_field), 
		data: {'turbo-frame': '_top' } do |f|%>

		<%= f.text_field :name %>
		<%= f.select :type, ["String", "Date", "Integer"], {} %>

		<%= f.submit(t("common.submit"), class: "btn btn-success")%>
	<% end %>
<% end %>