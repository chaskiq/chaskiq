<div class="px-4 py-5 sm:px-0 sm:py-0">
  <dl class="space-y-8 sm:space-y-0">
    <div class="sm:flex sm:space-x-6 sm:px-6 sm:py-5">
      <dt class="text-sm leading-5 font-medium text-gray-500 dark:text-gray-300 sm:w-40 sm:flex-shrink-0 lg:w-48">Last seen</dt>
      <dd class="mt-1 text-sm leading-5 text-gray-900 dark:text-gray-100 sm:mt-0 sm:col-span-2">
          <p><time datetime="1625288756000">an hour</time></p>
      </dd>
    </div>

    <% attributes = ['country', 'city', 'web_sessions', 'browser', 'os', 'tag_list' ] %>

    <% attributes.each do |attribute| %>
      <div class="sm:flex sm:space-x-6 sm:border-t sm:border-gray-200 dark:sm:border-gray-800 sm:px-6 sm:py-5">
        <dt class="text-sm leading-5 font-medium text-gray-500 dark:text-gray-300 sm:w-40 sm:flex-shrink-0 lg:w-48">
            <%= attribute %>
        </dt>
        <dd class="mt-1 text-sm leading-5 text-gray-900 dark:text-gray-100 sm:mt-0 sm:col-span-2">
            <%= @app_user.send(attribute.to_sym) %>
        </dd>
      </div>
    <% end %>
    
  </dl>
</div>