	<div class="align-middle min-w-full overflow-x-auto shadow overflow-hidden sm:rounded-lg">

		<div class="table min-w-full">
			<div class="table-header-group <%= 'hidden' if @headless %> border-b bg-gray-50 dark:bg-gray-900 dark:border-gray-600">
				<% @config[:columns].each do |c|%>
					<div class="table-cell px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
						<%= c[:field] %>
					</div>
				<% end %>
			</div>

			<div class="table-row-group bg-white dark:bg-black">

				<% @data.each_with_index do |record, index| %>
					<div
						class="table-row hover:bg-gray-50 dark:hover:bg-gray-800">
						<% @config[:columns].each do |c| %>
							<div class="table-cell align-middle px-6 py-4 whitespace-nowrap border-b border-gray-200 dark:border-gray-900">
								<%= record[c[:field]] %>
							</div>
						<% end %>
					</div>
				<% end %>
			</div>
		</div>

	</div>

	<% if @paginate.present? %>
		<div class="px-4 py-3 flex items-center justify-between sm:px-6--">
			<div class="flex-1 flex justify-between items-center">
				<%= @paginate %>
				<button class="transition duration-150 ease-in-out  css-1n1bu9a" disabled="">Prev</button>
				<p class="text-sm leading-5 text-gray-700 dark:text-gray-50">Showing<span class="font-medium ml-1 mr-1">1</span>to<span class="font-medium ml-1 mr-1">2</span>of<span class="font-medium ml-1 mr-1">29</span>results</p>
				<button class="transition duration-150 ease-in-out  css-1n1bu9a">Next</button>
			</div>
		</div>
	<% end %>

	<% if @paginate_meta.present? %>
		oli paginate meta here
	<% end %>
